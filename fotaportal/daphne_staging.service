[Unit]
Description=Daphne Server(Staging)
After=network.target

[Service]
Type=simple
User=www-data
Group=www-data
WorkingDirectory=/home/fotaportal/fotaportal_staging
ExecStart=/home/fotaportal/.virtualenvs/fotaportal_staging/bin/daphne --port 8001 fotaportal.asgi_staging:channel_layer
Restart=always

[Install]
WantedBy=multi-user.target
